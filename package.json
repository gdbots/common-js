{
  "name": "@gdbots/common",
  "version": "0.1.0",
  "description": "Common classes and utils for js projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/gdbots/common-js.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf lib",
    "test": "tape -r babel-register tests/**/*.test.js | faucet",
    "lint": "eslint src tests",
    "build:cjs": "rollup -c --format cjs --output ./lib/bundle.cjs.js",
    "build:es": "rollup -c --format es --output ./lib/bundle.es.js",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build:cjs && npm run build:es"
  },
  "main": "./lib/bundle.cjs.js",
  "module": "./lib/bundle.es.js",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "faucet": "^0.0.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "tape": "^4.6.3"
  }
}
