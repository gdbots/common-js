{
  "name": "@gdbots/common",
  "version": "0.1.0",
  "description": "Common classes and utils for js projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/gdbots/common-js.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf lib",
    "test": "tape -r babel-register tests/**/*.test.js | faucet",
    "lint": "eslint src tests",
    "build": "echo 'DISABLED BABEL_ENV=commonjs babel src --out-dir lib'",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "module": "./src/index.js",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "faucet": "^0.0.1",
    "rimraf": "^2.6.1",
    "tape": "^4.6.3"
  }
}
